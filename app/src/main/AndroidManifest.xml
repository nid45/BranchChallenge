<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.branchtechnicalchallenge">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_playstore"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_playstore"
        android:supportsRtl="true"
        android:theme="@style/Theme.BranchTechnicalChallenge">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.BranchTechnicalChallenge.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>